module RollingSum (rollingSum) where
{ rollingSum :: [Double] -> [Double]
; rollingSum xs = scan plus 0 xs
    where
      { plus x y = x + y
      }

; scan :: (Double -> Double -> Double) -> Double -> [Double] -> [Double]
; scan f x xs = ys
    where
      { ys = zipWith f xs (offset (0-1) ys)
      }
}
